  ],
  "license": "MIT License",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build": "yarn build:dev",
    "build:analyze": "env-cmd -e c,p,a yarn build:prod",
    "build:ci": "env-cmd -e c,ci next build",
    "build:dev": "env-cmd -e c,d next build",
    "build:prod": "env-cmd -e c,p next build",
    "build:test": "env-cmd -e c,t next build",
    "ci": "cross-env CI=true yarn ci:e2e && yarn ci:unit",
    "ci:cy": "env-cmd -e c,ci -x cypress run --config baseUrl=http://localhost:\\$PORT",
    "ci:e2e": "concurrently -k -s first \"yarn watch:ci\" \"yarn ci:cy\"",
    "ci:e2e:cov": "env-cmd -e c,ci echo 'ci:e2e:cov not implemented yet'",
    "ci:e2e:watch": "concurrently \"yarn watch:ci\" \"yarn ci:cy\"",
    "ci:unit": "echo 'ci:unit is not implemented yet'",
    "ci:unit:watch": "echo 'ci:unit:watch is not implemented yet'",
    "ci:watch": "yarn ci:e2e:watch && yarn ci:unit:watch",
    "clean": "rimraf .next/ build/ out/ target/",
    "deploy": "echo 'deploy is not implemented yet'",
    "dev": "next dev",
    "eslint:check": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "format": "yarn prettier:check",
    "format:fix": "yarn prettier:fix",
    "install:clean": "yarn clean && rimraf node_modules/ yarn.lock && yarn install",
    "lint": "yarn type:check && yarn eslint:check",
    "lint:fix": "yarn type:check && yarn eslint:fix",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "prettier:fix": "prettier --write \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "start": "yarn start:dev",
    "start:ci": "env-cmd -e c,ci -x cross-env PORT=\\$PORT next start",
    "start:dev": "env-cmd -e c,d -x cross-env PORT=\\$PORT next start",
    "start:prod": "env-cmd -e c,p -x cross-env PORT=\\$PORT next start",
    "start:test": "env-cmd -e c,t -x cross-env PORT=\\$PORT next start",
    "type:check": "tsc --noemit",
    "test": "yarn test:e2e && yarn test:unit",
    "test:cy": "env-cmd -e c,t -x cypress open --config baseUrl=http://localhost:\\$PORT",
    "test:e2e": "yarn build:test && concurrently -k -s first \"yarn start:test\" \"yarn test:cy\"",
    "test:e2e:cov": "echo 'test:e2e:cov not implemented yet'",
    "test:e2e:watch": "concurrently -k \"yarn watch:test\" \"yarn test:cy\"",
    "test:unit": "echo 'test:unit is not implemented yet'",
    "test:unit:watch": "echo 'test:unit:watch is not implemented yet'",
    "test:watch": "yarn test:e2e:watch && yarn test:unit:watch",
    "watch": "yarn watch:dev",
    "watch:ci": "env-cmd -e c,ci -x cross-env PORT=\\$PORT next dev",
    "watch:debug": "env-cmd -e c,d -x cross-env PORT=\\$PORT NODE_OPTIONS='--inspect' next dev",
    "watch:dev": "env-cmd -e c,d -x cross-env PORT=\\$PORT next dev",
    "watch:prod": "env-cmd -e c,p -x cross-env PORT=\\$PORT next dev",
    "watch:test": "env-cmd -e c,t -x cross-env PORT=\\$PORT next dev"
  },
  "dependencies": {
