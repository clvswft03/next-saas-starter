    "format:fix": "yarn prettier:fix",
    "install:clean": "yarn clean && rimraf node_modules/ yarn.lock && yarn install",
    "lint": "yarn type:check && yarn eslint:check",
    "lint:fix": "yarn type:check && yarn eslint:fix",
    "prepare": "husky install",
    "prettier:check": "prettier --ignore-path .eslintignore --check \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "prettier:fix": "prettier --ignore-path .eslintignore --write \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
