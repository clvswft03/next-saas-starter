{
  "name": "next-saas-starter",
  "version": "0.1.0",
  "description": "A modern design bootstrap starter for Next.js Application",
  "keywords": [
    "babel",
    "boilerplate",
    "bootstrap",
    "clipboard",
    "commitlint",
    "component",
    "components",
    "cypress",
    "design",
    "env",
    "eslint",
    "example",
    "front",
    "frontend",
    "fullpage",
    "fullpage.js",
    "fullpagejs",
    "git hooks",
    "landing page",
    "landing",
    "mdx",
    "next",
    "nextjs",
    "prettier",
    "react hooks",
    "react",
    "reactjs",
    "responsive",
    "ssr",
    "starter",
    "styled-components",
    "svg",
    "svgr",
    "swiper",
    "template",
    "test",
    "typescript",
    "ui",
    "webpack"
  ],
  "license": "MIT License",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build": "yarn build:prod",
    "build:anlz": "yarn _envx:z yarn _build",
    "build:debug": "yarn _envx:b yarn _build",
    "build:ci": "yarn _envx:ci yarn _build",
    "build:dev": "yarn _envx:d yarn _build",
    "build:prod": "yarn _envx:p yarn _build",
    "build:test": "yarn _envx:t yarn _build",
    "ci": "yarn ci:e2e && yarn ci:unit",
    "ci:check": "yarn ci:e2e:check && yarn ci:unit:check",
    "ci:e2e": "yarn _envx:ci yarn _conc:first \"yarn watch:ci\" \"yarn _cy:run\"",
    "ci:e2e:check": "yarn _envx:ci yarn _conc:first \"yarn watch:ci\" \"yarn _cy:run:headed\"",
    "ci:e2e:cov": "yarn _envx:ci echo-cli 'ci:e2e:cov is not implemented yet'",
    "ci:unit": "yarn _envx:ci echo-cli 'ci:unit is not implemented yet'",
    "ci:unit:check": "yarn _envx:ci echo-cli 'ci:unit:watch is not implemented yet'",
    "clean": "rimraf .next/ build/ out/ target/",
    "deploy": "echo-cli 'deploy is not implemented yet'",
    "format": "yarn _prettier:check",
    "format:fix": "yarn _prettier:fix",
    "install:clean": "yarn clean && rimraf node_modules/ yarn.lock && yarn install",
    "lint": "yarn type:check && yarn _eslint:check",
    "lint:fix": "yarn type:check && yarn _eslint:fix",
    "prepare": "husky install",
    "start": "yarn start:prod",
    "start:ci": "yarn _envx:ci yarn _start",
    "start:debug": "yarn _envx:b yarn _start",
    "start:dev": "yarn _envx:d yarn _start",
    "start:prod": "yarn _envx:p yarn _start",
    "start:test": "yarn _envx:t yarn _start",
    "test": "yarn test:e2e && yarn test:unit",
    "test:debug": "echo-cli 'test:debug is not implemented yet'",
    "test:e2e": "yarn _envx:t yarn _conc:first \"yarn watch:tina:test\" \"yarn _cy:run:headed\"",
    "test:e2e:cov": "echo-cli 'test:e2e:cov is not implemented yet'",
    "test:e2e:open": "yarn _conc:first \"yarn watch:tina:test\" \"yarn _cy:open\"",
    "test:e2e:watch": "yarn _envx:t yarn _conc:first \"yarn watch:tina:test\" \"yarn _cy:open\"",
    "test:unit": "echo-cli 'test:unit is not implemented yet'",
    "test:unit:watch": "echo-cli 'test:unit:watch is not implemented yet'",
    "test:watch": "yarn test:e2e:watch && yarn test:unit:watch",
    "tina-dev": "yarn tinacms server:start -c \"next dev\"",
    "tina-build": "yarn tinacms server:start -c \"next build\"",
    "tina-start": "yarn tinacms server:start -c \"next start\"",
    "type:check": "tsc --noemit",
    "watch": "yarn watch:prod",
    "watch:ci": "yarn watch:tina:ci",
    "watch:debug": "yarn watch:tina:debug",
    "watch:dev": "yarn watch:tina:dev",
    "watch:prod": "yarn watch:tina:prod",
    "watch:test": "yarn watch:tina:test",
    "watch:tina:ci": "yarn _envx:ci yarn _watch:tina",
    "watch:tina:debug": "yarn _envx:b yarn _watch:tina",
    "watch:tina:dev": "yarn _envx:d yarn _watch:tina",
    "watch:tina:prod": "yarn _envx:p yarn _watch:tina",
    "watch:tina:test": "yarn _envx:t yarn _watch:tina",
    "_build": "yarn tina-build",
    "_build:next": "next build",
    "_build:tina": "cross-env-shell ENV=\\$ENV yarn tinacms server:start -c \"yarn _build:next\"",
    "_conc:first": "concurrently -k -s first",
    "_cy:open": "yarn _wait:url && cypress open --browser \"$CY_BROWSER\" --config baseUrl=\"$HOST:$PORT\"",
    "_cy:run": "yarn _wait:url && cypress run --browser \"$CY_BROWSER\" --config baseUrl=\"$HOST:$PORT\"",
    "_cy:run:headed": "yarn _wait:url && cypress run --headed --browser \"$CY_BROWSER\" --config baseUrl=\"$HOST:$PORT\"",
    "_echo:port": "echo-cli PORT=$PORT",
    "_echo:url": "echo-cli URL=$URL",
    "_env:b": "env-cmd -e c,d,b",
    "_env:ci": "env-cmd -e c,t,ci",
    "_env:d": "env-cmd -e c,d",
    "_env:p": "env-cmd -e c,p",
    "_env:t": "env-cmd -e c,t",
    "_env:z": "env-cmd -e c,p,z",
    "_envx:b": "env-cmd -e c,d,b -x",
    "_envx:ci": "env-cmd -e c,t,ci -x",
    "_envx:d": "env-cmd -e c,d -x",
    "_envx:p": "env-cmd -e c,p -x",
    "_envx:t": "env-cmd -e c,t -x",
    "_envx:z": "env-cmd -e c,p,z -x",
    "_envxs:b": "env-cmd -e c,d,b -x --use-shell",
    "_envxs:ci": "env-cmd -e c,t,ci -x --use-shell",
    "_envxs:d": "env-cmd -e c,d -x --use-shell",
    "_envxs:p": "env-cmd -e c,p -x --use-shell",
    "_envxs:t": "env-cmd -e c,t -x --use-shell",
    "_envxs:z": "env-cmd -e c,p,z -x --use-shell",
    "_eslint:check": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "_eslint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "_prettier:check": "prettier --check \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "_prettier:fix": "prettier --write \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "_start": "yarn tina-start",
    "_start:next": "cross-env-shell PORT=\\$PORT next start",
    "_start:tina": "yarn tinacms server:start -c \"yarn _start:next\"",
    "_test:env:child": "cross-env-shell \"echo-cli Hello \\$NAME\"",
    "_test:env:greet": "cross-env-shell GREETING=Hi NAME=Joe \"echo-cli \\$GREETING && echo-cli \\$NAME\"",
    "_test:env:load": "yarn _test:env:set \"echo-cli Hello \\$NAME && echo-cli Hello \\$NAME\"",
    "_test:env:parent": "cross-env NAME=\"Joe\" yarn _test:env:child",
    "_test:env:set": "cross-env-shell NAME=\"Joe\"",
    "_wait:url": "echo-cli \"Waiting on $HOST:$PORT\" && wait-on \"$HOST:$PORT\"",
    "_watch": "cross-env-shell PORT=\\$PORT next dev",
    "_watch:tina": "yarn tinacms server:start -c \"yarn _watch\""
  },
  "dependencies": {
    "@accessible/drawer": "^3.0.2",
    "@babel/runtime": "^7.16.5",
    "@fullpage/react-fullpage": "^0.1.19",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@sendgrid/mail": "^7.4.7",
    "@svgr/webpack": "^5.5.0",
    "@tinacms/cli": "^0.58.5",
    "@tinacms/toolkit": "^0.56.4",
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/styled-components": "^5.1.18",
    "@types/swiper": "^5.4.3",
    "core-js": "^3.20.0",
    "css-in-js-media": "^2.0.1",
    "date-fns": "^2.24.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "next": "12.0.7",
    "nextjs-color-mode": "^1.0.4",
    "polished": "^4.1.3",
    "prism-react-renderer": "^1.2.1",
    "react": "17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.4",
    "react-is": "17.0.2",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-schemaorg": "^2.0.0",
    "react-share": "^4.4.0",
