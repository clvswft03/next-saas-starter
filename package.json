    "@types/cypress": "1.1.3",
    "all-contributors-cli": "6.20.0",
    "babel-plugin-module-resolver": "4.1.0",
    "concurrently": "6.5.1",
    "copy-webpack-plugin": "9.1.0",
    "cross-env": "7.0.3",
    "cypress": "9.1.1",
    "cz-conventional-changelog": "3.3.0",
    "env-cmd": "10.1.0",
    "eslint": "7.32.0",
