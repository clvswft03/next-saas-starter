{
  "name": "next-saas-starter",
  "version": "0.1.0",
  "description": "A modern design bootstrap starter for Next.js Application",
  "keywords": [
    "babel",
    "boilerplate",
    "bootstrap",
    "clipboard",
    "commitlint",
    "component",
    "components",
    "cypress",
    "design",
    "env",
    "eslint",
    "example",
    "front",
    "frontend",
    "fullpage",
    "fullpage.js",
    "fullpagejs",
    "git hooks",
    "landing page",
    "landing",
    "mdx",
    "next",
    "nextjs",
    "prettier",
    "react hooks",
    "react",
    "reactjs",
    "responsive",
    "ssr",
    "starter",
    "styled-components",
    "svg",
    "svgr",
    "swiper",
    "template",
    "test",
    "typescript",
    "ui",
    "webpack"
  ],
  "license": "MIT License",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build": "yarn build:dev",
    "build:analyze": "env-cmd -e c,p,a yarn build:prod",
    "build:ci": "env-cmd -e c,ci next build",
    "build:dev": "env-cmd -e c,d next build",
    "build:prod": "env-cmd -e c,p next build",
    "build:test": "env-cmd -e c,t next build",
    "ci": "cross-env CI=true yarn ci:e2e && yarn ci:unit",
    "ci:cy": "env-cmd -e c,ci -x cypress run --config baseUrl=http://localhost:\\$PORT",
    "ci:e2e": "concurrently -k -s first \"yarn watch:ci\" \"yarn ci:cy\"",
    "ci:e2e:cov": "env-cmd -e c,ci echo 'ci:e2e:cov not implemented yet'",
    "ci:e2e:watch": "concurrently \"yarn watch:ci\" \"yarn ci:cy\"",
    "ci:unit": "echo 'ci:unit is not implemented yet'",
    "ci:unit:watch": "echo 'ci:unit:watch is not implemented yet'",
    "ci:watch": "yarn ci:e2e:watch && yarn ci:unit:watch",
    "clean": "rimraf .next/ build/ out/ target/",
    "deploy": "echo 'deploy is not implemented yet'",
    "dev": "next dev",
    "eslint:check": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "format": "yarn prettier:check",
    "format:fix": "yarn prettier:fix",
    "install:clean": "yarn clean && rimraf node_modules/ yarn.lock && yarn install",
    "lint": "yarn type:check && yarn eslint:check",
    "lint:fix": "yarn type:check && yarn eslint:fix",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "prettier:fix": "prettier --write \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "start": "yarn start:dev",
    "start:ci": "env-cmd -e c,ci -x cross-env PORT=\\$PORT next start",
    "start:dev": "env-cmd -e c,d -x cross-env PORT=\\$PORT next start",
    "start:prod": "env-cmd -e c,p -x cross-env PORT=\\$PORT next start",
    "start:test": "env-cmd -e c,t -x cross-env PORT=\\$PORT next start",
    "test": "yarn test:e2e && yarn test:unit",
    "test:cy": "env-cmd -e c,t -x cypress open --config baseUrl=http://localhost:\\$PORT",
    "test:e2e": "yarn build:test && concurrently -k -s first \"yarn start:test\" \"yarn test:cy\"",
    "test:e2e:cov": "echo 'test:e2e:cov not implemented yet'",
    "test:e2e:watch": "concurrently -k \"yarn watch:test\" \"yarn test:cy\"",
    "test:unit": "echo 'test:unit is not implemented yet'",
    "test:unit:watch": "echo 'test:unit:watch is not implemented yet'",
    "test:watch": "yarn test:e2e:watch && yarn test:unit:watch",
    "type:check": "tsc --noemit",
    "watch": "yarn watch:dev",
    "watch:ci": "env-cmd -e c,ci -x cross-env PORT=\\$PORT next dev",
    "watch:debug": "env-cmd -e c,d -x cross-env PORT=\\$PORT NODE_OPTIONS='--inspect' next dev",
    "watch:dev": "env-cmd -e c,d -x cross-env PORT=\\$PORT next dev",
    "watch:prod": "env-cmd -e c,p -x cross-env PORT=\\$PORT next dev",
    "watch:test": "env-cmd -e c,t -x cross-env PORT=\\$PORT next dev"
  },
  "dependencies": {
    "@accessible/drawer": "^3.0.2",
    "@babel/runtime": "7.16.5",
    "@fullpage/react-fullpage": "0.1.19",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@sendgrid/mail": "^7.4.7",
    "@svgr/webpack": "^5.5.0",
    "@tinacms/cli": "^0.58.5",
    "@tinacms/toolkit": "^0.56.4",
    "core-js": "3.20.0",
    "css-in-js-media": "^2.0.1",
    "date-fns": "^2.24.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "next": "11.1.3-canary.54",
    "nextjs-color-mode": "^1.0.4",
    "polished": "^4.1.3",
    "prism-react-renderer": "^1.2.1",
    "react": "17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.4",
    "react-is": "17.0.2",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-schemaorg": "^2.0.0",
    "react-share": "^4.4.0",
    "reading-time": "^1.5.0",
    "schema-dts": "^1.0.0",
    "sharp": "^0.29.3",
    "styled-components": "^5.3.1",
    "swiper": "7.0.7",
    "tinacms": "^0.61.1",
    "url-loader": "^4.1.1",
    "use-clipboard-copy": "^0.2.0",
    "use-resize-observer": "^8.0.0",
    "webpack": "^5.65.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@next/bundle-analyzer": "^11.1.2",
    "@next/eslint-plugin-next": "12.0.7",
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.11",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/styled-components": "^5.1.14",
    "@types/swiper": "^5.4.3",
    "all-contributors-cli": "^6.20.0",
    "babel-plugin-module-resolver": "4.1.0",
    "concurrently": "6.5.1",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "7.0.3",
    "cypress": "9.1.1",
    "cz-conventional-changelog": "3.3.0",
    "echo-cli": "2.0.0",
    "env-cmd": "10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "husky": "7.0.4",
    "lint-staged": "12.1.3",
    "markdownlint": "0.24.0",
    "next-mdx-remote": "^3.0.5",
    "prettier": "2.5.1",
    "remark-breaks": "^3.0.1",
    "remark-external-links": "^9.0.1",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.0",
    "remark-sectionize": "^1.1.1",
    "remark-slug": "^7.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.4.2",
    "wait-on": "6.0.1"
  }
}
