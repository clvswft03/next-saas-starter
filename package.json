    "babel",
    "boilerplate",
    "bootstrap",
    "clipboard",
    "commitlint",
    "component",
    "components",
    "cypress",
    "design",
    "env",
    "eslint",
    "example",
    "front",
    "frontend",
    "fullpage",
    "fullpage.js",
    "fullpagejs",
    "git hooks",
    "landing page",
    "landing",
    "mdx",
    "next",
    "nextjs",
    "prettier",
    "react hooks",
    "react",
    "reactjs",
    "responsive",
    "ssr",
