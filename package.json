    "date-fns": "2.27.0",
    "gray-matter": "4.0.3",
    "lodash": "4.17.21",
    "next": "12.0.7",
    "nextjs-color-mode": "1.0.4",
    "polished": "4.1.3",
    "prism-react-renderer": "1.2.1",
    "react": "17.0.2",
